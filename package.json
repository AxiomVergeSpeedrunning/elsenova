{
  "license": "MIT",
  "scripts": {
    "build": "swc --copy-files --include-dotfiles --source-root ./src --source-maps inline -q src -d lib/",
    "prepublishOnly": "yarn build",
    "dev": "yarn build; node lib/index.js",
    "watch": "nodemon --exec 'yarn dev' -e '.js'",
    "start": "NODE_ENV=production node lib/index.js",
    "lint": "eslint ./src/",
    "fix": "eslint ./src/ --fix",
    "test": "babel-node src/test.js"
  },
  "nodemonConfig": {
    "ignore": [
      "lib/*"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.51",
    "@swc/core": "^1.2.108",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "regenerator-runtime": "^0.13.9"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^8.2.0",
    "every.js": "^0.0.5",
    "mongodb": "^3.6.6",
    "node-schedule": "^2.0.0",
    "pupa": "^3.1.0",
    "sequelize": "^6.9.0",
    "sqlite3": "^5.0.2",
    "whisparse": "^1.0.0"
  }
}
